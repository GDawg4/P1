COMPILER Aritmetica    

CHARACTERS 

letter = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz". 
digit = "0123456789" .
tab = CHR(9).
eol = CHR(10).

KEYWORDS

while = "while".
do = "do".
if = "if".
switch = "switch".

TOKENS

ident = letter{letter|digit} EXCEPT KEYWORDS.
number = digit{digit}.


END Aritmetica